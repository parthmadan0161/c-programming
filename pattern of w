// Online C compiler to run C program online
#include <stdio.h>

int main(){
    int i,j;
    for (i=1;i<=5;i++)
    {
        for(j=1;j<=13;j++)
        {
            if(i==j)
            printf(" *");
            else if((5<j<8)&&((i+j)==10)&&(i>2))
            printf(" *");
            else if((7<j<9)&&((j-i)==4)&&(i>2))
            printf(" *");
            else if((8<j<14)&&((j+i)==14))
            printf(" *");
            else 
            printf("  ");
        }
        printf("\n");
    }

    return 0;
}
