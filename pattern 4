#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int i,j,r,k;
    scanf("%d",&r);
    for(i=1;i<=r;i++)
    {
        for(j=i;j<r;j++)
        printf("%d",j);
        for(k=1;k<=i;k++)
        printf("%d",r);
        printf("\n");
    }
    return 0;
}
