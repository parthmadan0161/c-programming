#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int i,j,r,k;
    scanf("%d",&r);
    for(i=1;i<=r;i++)
    {
        if(i>1)
         for(k=i;k>=2;k--)
         printf("%d",k);
        for(j=1;j<=(r-i+1);j++)
        {
        printf("%d",j);
        }
        printf("\n");
    }
    return 0;
}
