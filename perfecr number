// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a,i,temp;
    printf("enter the number");
    scanf("%d",&a);
    for(i=1;i<a;i++)
    {
        if(a%i==0)
        temp=temp+i;
    }
    if(temp==a)
    printf("the no. is perfect number");
    else 
    printf("the number is not perfect number");
    return 0;
}
